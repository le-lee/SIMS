<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="layerWindow" id="student_edit_content">
		<div class="layerContent">
			<form style="margin:0;padding:0" id="studentEditForm">
				<table class="table" width="100%" border="0" cellspacing="0">
					<tr>
						<td class="label">学生姓名<label style="color:red !important;"> *</label></td>
						<td class="data fullw">
							<input name="studentName" class="m-input" placeholder="studentName" />
						</td>
					</tr>
					<tr>
						<td class="label">学号<label style="color:red !important;"> *</label></td>
						<td class="data fullw">
							<input name="studentNo" class="m-input" />
						</td>
					</tr>
					<tr>
						<td class="label">年级<label style="color:red !important;"> *</label></td>
						<td class="data fullw">
							<input name="grade" class="m-input" />
						</td>
					</tr>
					<tr>
						<td class="label">班级<label style="color:red !important;"> *</label></td>
						<td class="data fullw">
							<input name="class" class="m-input" />
						</td>
					</tr>
				</table>	
						
			</form>
		</div>
	</div>